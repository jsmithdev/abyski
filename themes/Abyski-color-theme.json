	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#676f7d"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.template"
			],
			"settings": {
				"foreground": "#e9c581"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#c678dd"
			}
		},
		{
